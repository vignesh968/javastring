import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
    Scanner s = new Scanner (System.in);
    String a = s.nextLine ();
    String b="";
      for (int i =a.length()-1;i>=0; i--)
      {
        b+=a.charAt(i);
      }
      if(a.equals(b))
      {
          System.out.println(a+" is a palindrome");
      }
      else{
           System.out.println(a+" is Not a palindrome");
      }
}
}
